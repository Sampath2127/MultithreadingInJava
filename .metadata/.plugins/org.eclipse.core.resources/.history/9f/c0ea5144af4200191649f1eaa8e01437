package com.lovecoding.multiplethreads;

public class Main {

	
	public static void main(String[] args) {
		
	}
}

class Countdown{
	
	public void countdown(){
	 String color;
	 
	 switch(Thread.currentThread().getName()){
	 
	 case "Thread 1":
		 color=ThreadColor.ANSI_CYAN;
		 break;
	 case "Thread 2":
		 color=ThreadColor.ANSI_CYAN;
		 break;
	 case "Thread 3":
		 color=ThreadColor.ANSI_CYAN;
		 break;
		 
	 
	 }
	}
}
